{
  "name": "streaming-into-the-void",
  "version": "1.0.0",
  "scripts": {
    "client:dev": "cd ../client && REACT_APP_STAGE=dev npm start",
    "server:dev": "nodemon index.js",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "server:prod": "NODE_ENV=prod node index.js",
    "start": "npm run server:prod"
  },
  "dependencies": {
    "add-cors-to-couchdb": "0.0.6",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-pouchdb": "^4.1.0",
    "node-cron": "^2.0.3",
    "nodemon": "^1.18.4",
    "pouchdb-adapter-memory": "^7.0.0",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-node": "^7.0.0",
    "pouchdb-upsert": "^2.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "shortid": "^2.2.13"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
